<resources>

    <!--Customize here the subject of your application-->
    <style name="myTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
    </style>

    <!--Here will be the theme of the class that will extends NavigationLiveo-->
    <style name="MainActivityTheme" parent="MaterialNavigationDrawerTheme.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/statusbar</item>
        <item name="colorAccent">@color/accent</item>

        <item name="drawerType">@integer/DRAWERTYPE_IMAGE</item>
    </style>


</resources>
